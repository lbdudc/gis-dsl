{
  "author": "Álex Cortiñas",
  "dependencies": {
    "mocha": "^10.2.0"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "strip-bom": "^5.0.0"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{json,md,yml}": "prettier --write",
    "*.{js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "main": "src/index.js",
  "name": "gisdsl",
  "scripts": {
    "build-grammar": "antlr -Dlanguage=JavaScript -lib grammar -o src/lib -visitor -Xexact-output-dir grammar/GISGrammar.g4",
    "lint": "npm-run-all lint_eslint lint_prettier",
    "lint_eslint": "eslint --ext js,vue --fix src test",
    "lint_prettier": "prettier --write --ignore-unknown src test",
    "prepare": "husky install",
    "test": "mocha --ui qunit"
  },
  "type": "module",
  "version": "0.1.0"
}
